RewriteEngine On

# Define a base de reescrita
RewriteBase /ProjetoAutomacaoLeitos/AreaDeTrabalho/public/

# Não reescreve se o arquivo ou diretório já existir
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

# Redireciona tudo para index.php
RewriteRule ^(.*)$ index.php [QSA,L]
